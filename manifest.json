{
    "manifest_version": 3,
    "name": "LeetCode Notes",
    "description": "Chrome extension to add LeetCode coding solutions to Google Sheets",
    "version": "1.0.0",
    "icons": {
      "16": "icons/16.png",
      "32": "icons/38.png",
      "48": "icons/48.png",
      "128": "icons/128.png"
    },
    "permissions": [
      "activeTab",
      "storage",
      "identity",
      "https://*.googleapis.com/",
      "https://*.google.com/"
    ],
    "content_scripts": [
      {
        "matches": [
          "https://leetcode.com/problems/*"
        ],
        "js": [
          "content.js"
        ]
      }
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_icon": {
        "16": "icons/16.png",
        "32": "icons/38.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
      },
      "default_popup": "popup.html"
    }
}